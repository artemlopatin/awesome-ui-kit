{
  "name": "awesome-ui-kit",
  "version": "0.0.1",
  "description": "Awesome UI KIT",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "start-storybook -p 6006",
    "docs:clear": "rimraf ./docs",
    "docs:build": "build-storybook -o ./docs",
    "docs:git-add": "git add ./docs",
    "docs:refresh": "npm run docs:clear && npm run docs:build && npm run docs:git-add"
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo 'npm run docs:refresh'"
    }
  },
  "keywords": [
    "gui",
    "ui",
    "kit"
  ],
  "author": "Artem Lopatin",
  "license": "MIT",
  "dependencies": {
    "@types/classnames": "^2.2.10",
    "classnames": "^2.2.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/preset-typescript": "^1.2.2",
    "@storybook/react": "^5.3.18",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.3",
    "eslint": "^6.8.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "husky": "^4.2.5",
    "rimraf": "^3.0.2",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^6.2.2",
    "typescript": "^3.9.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
